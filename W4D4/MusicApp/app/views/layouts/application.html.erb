<!DOCTYPE html>
<html>
<head>
  <title>MusicApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
  <% if logged_in? %>
    <%= current_user.email %>
    <%= button_to "Log out", session_url, method: :delete %>
  <% end %>
</header>

<%= yield %>

</body>
</html>
